	{*================== BEGIN BASE CSS STYLE ==================*}

{*	{var $production = true}*}

	{block css}
	{if $production}
		<link data-pf-placeholder="head" rel="stylesheet" type="text/css" media="screen" href="{$basePath}/resources/pexesoModule/main-pexeso.min.css?rel=1573206067524">
	{else}
		{*<!-- build:css {$basePath}/resources/pexesoModule/main-pexeso.min.css -->*}
		<link rel="stylesheet" type="text/css" media="screen" href="{$basePath}/resources/pexesoModule/animate.css/animate.min.css">
		<link rel="stylesheet" type="text/css" media="screen" href="{$basePath}/resources/pexesoModule/select2/css/select2.css">
		<link n:if="$production" rel="stylesheet" type="text/css" media="screen" href="{$basePath}/resources/pexesoModule/pexeso.css">
		{*<!-- endbuild -->*}
		<link n:if="!$production" data-pf-placeholder="head" rel="stylesheet" type="text/css" media="screen" href="{$basePath}/resources/pexesoModule/pexeso.css">
	{/if}
{*	<link data-pf-placeholder="head" rel="stylesheet" type="text/css" media="screen" href="{$basePath}/resources/pexesoModule/themes/{$themeName}.css?v={$themeVersion}">*}
	{/block}

	{block head}
	{include "#gtm-head.latte"}
