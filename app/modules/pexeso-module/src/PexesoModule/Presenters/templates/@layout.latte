<body class="_debug">

	{block menu}
	{if $appLayout}
		{include parent}
		<li><a n:href="switchAppLayout!">Vzhled soutěže</a></li>
	{/if}
	{/block}


	<div n:block="body-html" data-pf-placeholder="content" data-pf-sortorder="1" data-pf-options="innerhtml">
		{*{include "#gtm-body.latte"}*}
		<div id="soutez" class="{$pageClass}" role="main" data-pf-proxy-url="" data-pf-proxy-base-url="{$proxyBaseUrl}" data-app-base-url="{link Homepage:}" data-logged-in="{$user->isLoggedIn() ? 'true' : 'false'}">
			{snippet content}
				{include content}
			{/snippet}
		</div>
	</div>


	{block scripts}
{*		<script type="text/javascript" _data-pf-placeholder="scripts" src="https://consumer.procampaignapi.com/Scripts/jquery-3.1.1.min.js"></script>*}
		<script n:if="!Devrun\CmsModule\Utils\Common::isAdminRequest()" type="application/javascript" data-pf-placeholder="scripts" src="{$basePath}/resources/pexesoModule/jquery/jquery.min.js"></script>
		<script n:if="!Devrun\CmsModule\Utils\Common::isAdminRequest()" type="text/javascript" data-pf-placeholder="scripts" src="https://consumer.procampaignapi.com/Scripts/json2.min.js"></script>
