			<script data-pf-placeholder="scripts" type="application/javascript" src="{$basePath}/resources/pexesoModule/modernizr.js"></script>
			<script data-pf-placeholder="scripts" type="application/javascript" src="{$basePath}/resources/pexesoModule/page.js"></script>
			<script data-pf-placeholder="scripts" type="application/javascript" src="{$basePath}/resources/pexesoModule/pexeso.js"></script>
