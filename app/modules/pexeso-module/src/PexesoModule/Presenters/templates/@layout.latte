	{block extraScripts}{/block}

	<script n:if="!Devrun\CmsModule\Utils\Common::isAdminRequest()" data-pf-placeholder="scripts">
		(function($, undefined) {

			ProCampaignForm.init(
				{
					'package': {$package->name},
					'transactionName': {_site.transaction.$package->name.name},
					'identLong': {_site.transaction.$package->name.identLong},
					{*'logUrl': {link log!}*}
				}
			);

			$(function () {
				{ifset $scroll}
				{if $scroll}
					$.smoothScroll({ scrollTarget: '#soutez' });
				{/if}
				{/ifset}
				{ifset $redirect}
					Page.switchPage('.redirect', event);
					console.log("redirect signal");
				{/ifset}
			});
		})(jQuery); //jNIV  test
	</script>

</body>
</html>
